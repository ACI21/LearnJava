/**
 * 
 */
package basicos;

/**
 * 
 */
public class humano {
	
	private String nombre;
	private int edad;
	private String extremidades;
	
	public humano() {
		super();
	}
	
	public humano(String nombre, int edad) {
		super();
		this.nombre = nombre;
		this.edad = edad;
		this.extremidades = null;
	}
	
	public humano(String nombre, int edad, String extremidades) {
		super();
		this.nombre = nombre;
		this.edad = edad;
		this.extremidades = extremidades;
	}
	
	
}
